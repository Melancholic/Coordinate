<% case type%>
  <% when 'error'%>
     <%= render 'shared/modals/error', messege:messege %>
  <% when 'info'%>
     <%= render 'shared/modals/info', messege:messege %>
  <% when 'warning'%>
     <%= render 'shared/modals/warning', messege:messege %>
  <% when 'success'%>
     <%= render 'shared/modals/success', messege:messege %>
  <% when 'notice'%>
     <%= render 'shared/modals/default', messege:messege %>  
<%end%>

